{
  "name": "tool-manage",
  "version": "0.0.1",
  "description": "dashboard tool",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "test": "node index",
    "dev": "set NODE_ENV=development&& node dist/index",
    "prod": "set NODE_ENV=production&& node dist/index",
    "build-dev": "set NODE_ENV=development&& babel src --out-dir dist",
    "coppy": "babel src/views --out-dir dist/views --source-maps inline --copy-files",
    "coppy-webpack": "babel src/webpack --out-dir dist/webpack --source-maps inline --copy-files",
    "build": "babel src --out-dir dist --source-maps inline",
    "build1": "set NODE_ENV=production&& babel src --out-file dist/index.js",
    "client-prod": "set NODE_ENV=production&& webpack --config ./dist/webpack/index.js -p",
    "start-prod": "set NODE_ENV=production&& npm run clean & npm run build & npm run coppy & npm run client-prod & npm run prod",
    "start": "set NODE_ENV=development&& nodemon --exec babel-node src/index.js",
    "client": "webpack --config ./src/webpack/index.js",
    "start-prod1": "set NODE_ENV=production&& npm run clean & npm run build1 & npm run coppy & npm run prod"
  },
  "author": "",
  "license": "ISC",
