{
  "name": "tool-manage",
  "version": "0.0.1",
  "description": "dashboard tool",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=development&& nodemon --exec babel-node src/index.js",
    "build": "webpack --config ./src/webpack/index.js",
    "coppy-views": "babel src/views -d public/views --source-maps inline --copy-files",
    "coppy-webpack": "babel src/webpack -d public/webpack --source-maps inline --copy-files",
    "coppy-client": "babel src/client -d public/client --source-maps inline --copy-files",
    "coppy": "npm run coppy-views & npm run coppy-webpack &  npm run coppy-client",
    "clean-public": "rm -rf public",
    "clean-client": "rm -rf public/client public/webpack public/views",
    "build-server": "webpack --config ./src/webpack/server.js",
    "prod-run-webpack": "set NODE_ENV=production&& webpack --config ./public/webpack/index.js",
    "start-prod": "set NODE_ENV=production&& node public/server",
    "build-prod": "npm run clean-public & npm run build-server & npm run coppy & npm run prod-run-webpack & npm run clean-client",
    "prod": "npm install --production & npm run start-prod"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.32",
    "body-parser": "^1.19.0",
    "clsx": "^1.0.4",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "log4js": "^5.3.0",
    "mini-css-extract-plugin": "^0.8.0",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "qs": "^6.9.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-hooks-usemodel": "0.0.4",
    "react-router-dom": "^5.1.2",
    "react-spring": "^8.0.27",
    "webpack": "^4.41.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.7.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-minify": "^0.5.1",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "file-loader": "^4.2.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "webpack-cli": "^3.3.10"
  }
}
